    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "debug",
        "runtimeVersion": "default",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/index.js",
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/nodemon",
        "runtimeArgs": ["--exec", "babel-node"],
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_ENV": "env",
          "TZ": "UTC"
        }
      }
    ]
  }
  